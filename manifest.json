{
    "author": "Tyler Leach",
    "version": "1.0.0",
    "background": {
        "persistent": false,
        "scripts": [
            "js/background.js"
        ]
    },
    "icons": { 
        "16": "healthmemo.png",
        "48": "healthmemo.png",
        "128": "healthmemo.png" },
    "browser_action": {
        "default_title": "HealthMemmo",
        "setBadgeText": "1"
    },
    "name": "HealthMemmo",
    "permissions": [
        "alarms",
        "cookies",
        "notifications",
        "tabs",
        "http://*/*",
        "https://*/*",
        "contextMenus",
        "storage",
        "activeTab",
        "background",
        "nativeMessaging"
    ],
    "content_security_policy": "script-src 'self' https://code.jquery.com/jquery-3.4.1.min.js; object-src 'self'",
    "manifest_version": 2,
    "short_name": "hm"
}